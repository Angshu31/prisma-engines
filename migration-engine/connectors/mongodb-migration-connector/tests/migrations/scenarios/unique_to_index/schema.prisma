 datasource db {
   provider = "mongodb"
   url      = "mongodb://"
 }

 generator js {
   provider        = "prisma-client-js"
   previewFeatures = ["extendedIndexes", "fullTextIndex", "mongodb"]
 }

 model User {
     id Int @id
     firstName String
     lastName String
     age Int

    @@index([firstName, lastName, age], map: "UserExistingIndex")
 }

 model Post {
     id Int @id
     title String

     @@index([title])
 }

