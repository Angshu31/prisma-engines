 datasource db {
    provider = "mongodb"
    url = env("TEST_DATABASE_URL")
 }

 generator js {
    provider = "prisma-client-js"
    previewFeatures = ["mongodb"]
 }

 model User {
     id Int @id
     firstName String
     lastName String
     age Int

    @@unique([firstName, lastName, age], map: "UserExistingIndex")
 }

 model Post {
     id Int @id
     title String

     @@index([title])
}

