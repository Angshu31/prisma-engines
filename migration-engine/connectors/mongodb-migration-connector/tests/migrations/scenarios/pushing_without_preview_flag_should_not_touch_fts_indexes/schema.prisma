 datasource db {
   provider = "mongodb"
   url      = "mongodb://"
 }

 generator js {
   provider        = "prisma-client-js"
   previewFeatures = ["extendedIndexes", "fullTextIndex", "mongodb"]
 }

 model User {
   id        String @id @map("_id") @db.ObjectId
   age       Int
   firstName String
   lastName  String
   location  String
 }
