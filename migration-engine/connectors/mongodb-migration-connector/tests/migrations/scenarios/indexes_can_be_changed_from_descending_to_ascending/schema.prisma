 datasource db {
    provider = "mongodb"
    url = env("TEST_DATABASE_URL")
 }

 generator js {
    provider = "prisma-client-js"
    previewFeatures = ["mongodb", "extendedIndexes"]
 }

 model User {
   id Int @id
   firstName String
   lastName String
   age Int

   @@index([firstName, lastName, age(sort: Desc)])
 }
