 datasource db {
    provider = "mongodb"
    url = env("TEST_DATABASE_URL")
 }

 generator js {
    provider = "prisma-client-js"
    previewFeatures = ["mongodb"]
 }

 model User {
     id Int @id
     firstName String @map("fst_name")
     lastName String @map("lst_name")
     age Int

    @@index([firstName, lastName, age])
    @@map("users")
 }

