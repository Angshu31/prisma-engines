datasource chinook {
  provider = "postgres"
  url      = "postgresql://postgres:prisma@localhost/postgres"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  posts Post[]
}

model Post {
  id     Int    @id @default(autoincrement())
  title  String
  user   User   @relation(fields: [userId], references: [id])
  userId Int
}
